[["\u30b5\u30b1\u30cf\u30f3\u30bf\u30fc\u3000\u30b0\u30ea\u30ba\u30ea\u30fc\u3055\u3093", "Salmon Hunter Grizzly-san"], ["\u304a\u306f\u3088\u3046", "Good morning."], ["\u304a\u306f\u3088\u3046\u3000\u30d1\u30f3\u30c0\u304f\u3093", "Good morning, Panda."], ["\u3042\u308c\uff01\uff1f\u3000\u3053\u308c\u3000\u306a\u3093\u306e\u8377\u7269\uff1f", "Huh!? What's this package?"], ["\u304a\u51fa\u304b\u3051\u306e\u6e96\u5099", "Getting ready to go out"], ["\u3048\u3063\uff01\uff1f\u3000\u51fa\u304b\u3051\u3061\u3083\u3046\u306e\uff1f \u30ab\u30d5\u30a7\u306f\uff1f", "Huh!? You're going out? What about the caf\u00e9?"], ["\u4eca\u65e5\u306f\u304a\u4f11\u307f", "Today is a day off."], ["\u305b\u3063\u304b\u304f\u3000\u65e9\u8d77\u304d\u3067\u304d\u305f\u304b\u3089\u30e2\u30fc\u30cb\u30f3\u30b0\u98df\u3079\u3088\u3046\u3068\u601d\u3063\u305f\u306e\u306b", "I managed to wake up early, so I was thinking of having breakfast."], ["\u3054\u3081\u3093\u306d", "Sorry."], ["\u3069\u3053\u884c\u304f\u306e\uff1f", "Where are you going?"], ["\u4eca\u65e5\u306f\u306d\u2026", "Today, I'm..."], ["\u30b7\u30ed\u30af\u30de\uff01 \u7528\u610f\u306f\u3067\u304d\u3066\u3093\u3060\u308d\u3046\u306a\uff1f", "Polar Bear! You're ready, right?"], ["\u8377\u7269\u306f\u3053\u308c\u3067\u5168\u90e8\u304b\uff1f", "Is this all the luggage?"], ["\u3046\u3093", "Yeah"], ["\u30b0\u30ea\u30ba\u30ea\u30fc\u3055\u3093\u304a\u306f\u3088\u3046", "Good morning, Mr. Grizzly."], ["\u304a\u3046", "Yeah"], ["\u30b7\u30ed\u30af\u30de\u304f\u3093\u3068\u3069\u3053\u884c\u304f\u306e\uff1f", "Where are you going with Shirokuma?"], ["\u3042\u3041\u3000\u3061\u3087\u3063\u3068\u98df\u6750\u3092\u306a", "Oh, just going to get some groceries."], ["\u6599\u7406\u306e\u6750\u6599\u3092\u767a\u6398\u3057\u306b", "To dig up some cooking ingredients."], ["\u767a\u6398\uff1f \u697d\u3057\u305d\u3046\uff01", "Excavating? Sounds fun!"], ["\u50d5\u3082\u884c\u304d\u305f\u3044\u2026", "I want to go too..."], ["\u904a\u3073\u3058\u308